    "@types/react-router-dom": "^5.3.3",
    "@types/react-router-hash-link": "^2.4.5",
    "@types/react-table": "^7.7.14",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.3",
    "c8": "^7.13.0",
    "cspell": "^6.26.3",
    "d3": "^7.8.2",
    "data-forge": "^1.9.6",
    "env-cmd": "^10.1.0",
