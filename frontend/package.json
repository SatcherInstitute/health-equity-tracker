    "typeface-hind": "^1.1.13",
    "typeface-montserrat": "^1.1.13",
    "use-react-screenshot": "^3.0.0",
    "vega": "^5.22.1",
    "vega-lite": "^5.6.1",
    "vite": "^4.1.4",
    "vite-plugin-babel": "^1.1.3",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.7",
    "webpack": "^5.84.1"
  },
  "scripts": {
    "build:deploy_preview": "env-cmd -f .env.deploy_preview npm run build",
    "build:development": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build": "tsc && vite build",
    "cspell": "cspell '../**/*.{ts,tsx,scss,md,py}' --no-progress --no-must-find-files",
    "dev": "env-cmd -f .env.development npm run start -- --host",
    "e2e": "npx playwright install chromium && npx playwright test --project=E2E",
    "playwright": "npx playwright install chromium && npx playwright test",
    "serve": "vite preview",
    "start:development": "env-cmd -f .env.development npm run start",
    "start": "vite",
    "storybook:development": "env-cmd -f .env.development npm run storybook",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "tsc": "npx tsc --noEmit",
    "url": "npx playwright install chromium && npx playwright test --project=URL"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=6.14.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": "bash -c tsc --noEmit",
    "*": "npm run cspell",
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@vitest/coverage-c8": "^0.29.8",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "typescript": "^5.0.3",
    "vitest-canvas-mock": "^0.2.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
