  "scripts": {
    "build:deploy_preview": "env-cmd -f .env.deploy_preview npm run build",
    "build:development": "env-cmd -f .env.development npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build": "tsc && vite build",
    "cspell": "cspell '../**/*.{ts,tsx,scss,md,py}' --no-progress --no-must-find-files --gitignore",
    "dev": "env-cmd -f .env.development npm run start -- --host",
    "e2e-staging": "npx playwright install chromium && npx playwright test --project=E2E_STAGING --workers 5",
    "e2e-prod": "npx playwright install chromium && npx playwright test --project=E2E_PROD --workers 5",
    "e2e-local": "npx playwright install chromium && npx playwright test --project=E2E_LOCAL --workers 2",
    "e2e-deploy-preview": "npx playwright install chromium && npx playwright test --project=E2E_DEPLOY_PREVIEW --workers 5",
    "e2e-new": "npx playwright codegen http://localhost:3000",
    "preview": "npm run build:development && vite preview",
    "start:deploy_preview": "env-cmd -f .env.deploy_preview npm run start",
    "start:development": "env-cmd -f .env.development npm run start",
    "start": "vite",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "tsc": "npx tsc --noEmit",
    "url": "npx playwright install chromium && npx playwright test --project=URL"
  },
  "engines": {
    "node": ">=19",
    "npm": ">=6.14.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": "bash -c tsc --noEmit",
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,md,html}": [
      "prettier --write"
    ]
  }
}